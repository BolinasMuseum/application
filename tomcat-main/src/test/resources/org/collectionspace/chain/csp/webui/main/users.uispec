{
    "details": {
        ".csc-user-userName": "${fields.screenName}",
        ".csc-user-status": {
            "default": "0",
            "optionnames": ["Active", "Inactive"],
            "optionlist": ["active", "inactive"],
            "selection": "${fields.status}"
        },
        ".csc-users-createdAt": "${fields.createdAt}",
        ".csc-accountrole-roles-row": {
            "children": [{
                ".csc-accountrole-account-rolename": "${fields.role.0.roleName}",
                ".csc-accountrole-role-roleid": "${fields.role.0.roleId}"
            }]
        },
        ".csc-accountrole-account-accountid": "${fields.accountId}",
        ".csc-user-password": "${fields.password}",
        ".csc-user-userID": "${fields.userId}",
        ".csc-user-email": "${fields.email}"
    },
    "list": {
        ".csc-recordList-row:": {
            "children": [{
                ".csc-user-userList-status": "${items.0.status}",
                ".csc-user-userList-csid": "${items.0.csid}",
                ".csc-user-userList-name": "${items.0.screenName}"
            }]
        }
    }
}