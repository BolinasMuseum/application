                            "expander": {
                                "tree": {
                                    ".csc-exhibition-exhibitionReference": {
                                        "decorators": [
                                            {
                                                "func": "cspace.autocomplete",
                                                "options": {
                                                    "queryUrl": "../../../tenant/core/exhibition/autocomplete/exhibitionReference",
