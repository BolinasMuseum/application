{
    "sidebar": {
        "relatedProcedures": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-summary": "${items.0.summary}",
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    },
                    ".csc-related-recordtype": "${items.0.recordtype}"
                }]
            }
        },
        "relatedObjects": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-summary": "${items.0.summary}",
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    }
                }]
            }
        },
        "termsUsed": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    },
                    ".csc-related-recordtype": "${items.0.recordtype}",
                    ".csc-related-field": "${items.0.sourceFieldName}"
                }]
            }
        }
    },
    "recordEditor": {
        ".csc-personAuthority-nameNote": "${fields.nameNote}",
        ".csc-personAuthority-deathPlace": "${fields.deathPlace}",
        ".csc-personAuthority-birthDate": "${fields.birthDate}",
        ".csc-personAuthority-status": {
            "default": "0",
            "optionnames": ["Provisional", "Under Review", "Accepted", "Rejected"],
            "optionlist": ["provisional", "under review", "accepted", "rejected"],
            "selection": "${fields.termStatus}"
        },
        ".csc-personAuthority-name": "${fields.name}",
        ".csc-contactAuthority-web": "${fields.web}",
        ".csc-personAuthority-nameAdditions": "${fields.nameAdditions}",
        ".csc-personAuthority-gender": {
            "optionnames": ["Male", "Female"],
            "optionlist": ["male", "female"],
            "selection": "${fields.gender}"
        },
        ".csc-contactAuthority-addressType": "${fields.addressType}",
        ".csc-contactAuthority-faxNumber": "${fields.faxNumber}",
        ".csc-personAuthority-occupation": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.occupations",
                    "protoTree": {
                        ".csc-personAuthority-occupation": "${fields.occupations.0.occupation}"
                    }
                }
            }]
        },
        ".csc-personAuthority-middleName": "${fields.middleName}",
        ".csc-personAuthority-surName": "${fields.surName}",
        ".csc-contactAuthority-telephoneNumber": "${fields.telephoneNumber}",
        ".csc-personAuthority-salutation": {
            "optionnames": ["Dear", "Hello", "To"],
            "optionlist": ["dear", "hello", "to"],
            "selection": "${fields.salutation}"
        },
        ".csc-personAuthority-schoolOrStyle": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.schoolsOrStyles",
                    "protoTree": {
                        ".csc-personAuthority-schoolOrStyle": "${fields.schoolsOrStyles.0.schoolOrStyle}"
                    }
                }
            }]
        },
        ".csc-personAuthority-displayName": "${fields.displayName}",
        ".csc-personAuthority-title": {
            "optionnames": ["Mr", "Mrs", "Ms", "Miss", "Dr", "Professor", "Sir", "Dame", "Baron", "Baroness", "Lord", "Lady"],
            "optionlist": ["Mr", "Mrs", "Ms", "Miss", "Dr", "Professor", "Sir", "Dame", "Baron", "Baroness", "Lord", "Lady"],
            "selection": "${fields.title}"
        },
        ".csc-contactAuthority-addressPlace": "${fields.addressPlace}",
        ".csc-personAuthority-initials": "${fields.initials}",
        ".csc-personAuthority-birthDate-container": {
            "decorators": [{
                "func": "cspace.datePicker",
                "type": "fluid"
            }]
        },
        ".csc-personAuthority-group": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.groups",
                    "protoTree": {
                        ".csc-personAuthority-group": "${fields.groups.0.group}"
                    }
                }
            }]
        },
        ".csc-personAuthority-foreName": "${fields.foreName}",
        ".csc-personAuthority-deathDate": "${fields.deathDate}",
        ".csc-personAuthority-nationality": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.nationalities",
                    "protoTree": {
                        ".csc-personAuthority-nationality": "${fields.nationalities.0.nationality}"
                    }
                }
            }]
        },
        ".csc-personAuthority-birthPlace": "${fields.birthPlace}",
        ".csc-personAuthority-bioNote": "${fields.bioNote}",
        ".csc-contactAuthority-email": "${fields.email}",
        ".csc-person-shortIdentifier": "${fields.shortIdentifier}",
        ".csc-personAuthority-deathDate-container": {
            "decorators": [{
                "func": "cspace.datePicker",
                "type": "fluid"
            }]
        }
    },
    "titleBar": {
        ".csc-titleBar-birthDate": "${fields.birthDate}",
        ".csc-titleBar-displayName": "${fields.displayName}",
        ".csc-titleBar-deathDate": "${fields.deathDate}"
    }
}