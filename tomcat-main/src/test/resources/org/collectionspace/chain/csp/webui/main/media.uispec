{
    "recordEditor": {
        ".csc-media-creator": {
            "decorators": [{
                "func": "cspace.autocomplete",
                "type": "fluid",
                "options": {
                    "queryUrl": "../../chain/media/autocomplete/creator",
                    "vocabUrl": "../../chain/media/source-vocab/creator"
                }
            }],
            "value": "${fields.creator}"
        },
        ".csc-media-dates": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.dates",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-date": "${{row}.date}",
                                        ".csc-media-date-container": {
                                            "decorators": [{
                                                "func": "cspace.datePicker",
                                                "type": "fluid"
                                            }]
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.dates"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-media-coverage": "${fields.coverage}",
        ".csc-blobs-uri": "${fields.blobs.0.uri}",
        ".csc-media-relation": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.relations",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-relation": "${{row}.relation}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.relations"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-media-srcUri": "${fields.srcUri}",
        ".csc-media-format": "${fields.format}",
        ".csc-media-contributor": {
            "decorators": [{
                "func": "cspace.autocomplete",
                "type": "fluid",
                "options": {
                    "queryUrl": "../../chain/media/autocomplete/contributor",
                    "vocabUrl": "../../chain/media/source-vocab/contributor"
                }
            }],
            "value": "${fields.contributor}"
        },
        ".csc-blobs-digest": "${fields.blobs.0.digest}",
        ".csc-media-dateModified": "${fields.dateModified}",
        ".csc-media-dateCreated": "${fields.dateCreated}",
        ".csc-blobs-mimeType": "${fields.blobs.0.mimeType}",
        ".csc-blobs-imgThumb": "${fields.blobs.0.imgThumb}",
        ".csc-blobs-name": "${fields.blobs.0.name}",
        "expander": [{
            "repeatID": ".csc-blobs-dimensionGroup",
            "tree": {
                ".csc-blobs-measurementMethod": "${{row}.measurementMethod}",
                ".csc-blobs-value": "${{row}.value}",
                ".csc-blobs-measuredBy": "${{row}.measuredBy}",
                ".csc-blobs-valueQualifier": "${{row}.valueQualifier}",
                ".csc-blobs-valueDate": "${{row}.valueDate}",
                ".csc-blobs-dimension": "${{row}.dimension}",
                ".csc-blobs-measurementUnit": "${{row}.measurementUnit}"
            },
            "type": "fluid.renderer.repeat",
            "pathAs": "row",
            "controlledBy": "fields.blobs.0.dimensionGroup"
        }, {
            "condition": {
                "funcName": "cspace.mediaUploader.assertBlob",
                "args": "${fields.blobCsid}"
            },
            "trueTree": {
                ".csc-media-blobs": {}
            },
            "type": "fluid.renderer.condition",
            "falseTree": {
                ".csc-media-blobs": {
                    "decorators": {
                        "classes": "hidden",
                        "type": "addClass"
                    }
                }
            }
        }],
        ".csc-media-updatedAt": "${fields.updatedAt}",
        ".csc-media-location": "${fields.location}",
        ".csc-media-rightsHolder": {
            "decorators": [{
                "func": "cspace.autocomplete",
                "type": "fluid",
                "options": {
                    "queryUrl": "../../chain/media/autocomplete/rightsHolder",
                    "vocabUrl": "../../chain/media/source-vocab/rightsHolder"
                }
            }],
            "value": "${fields.rightsHolder}"
        },
        ".csc-media-tenantID": "${fields.tenantID}",
        ".csc-blobs-imgOrig": "${fields.blobs.0.imgOrig}",
        ".csc-media-copyrightStatement": "${fields.copyrightStatement}",
        ".csc-media-type": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.types",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-type": {
                                            "default": "0",
                                            "optionnames": ["Dataset", "Document", "Moving Image", "Please select a value", "Sound", "Still Image"],
                                            "optionlist": ["", "dataset", "document", "moving_image", "sound", "still_image"],
                                            "selection": "${{row}.type}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.types"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-media-source": "${fields.source}",
        ".csc-blobs-updatedAt": "${fields.blobs.0.updatedAt}",
        ".csc-media-externalMediaUrl": "${fields.externalMediaUrl}",
        ".csc-media-subject": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.subjects",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-subject": "${{row}.subject}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.subjects"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-blobs-encoding": "${fields.blobs.0.encoding}",
        ".csc-blobs-createdAt": "${fields.blobs.0.createdAt}",
        ".csc-media-dimensionGroup": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.dimensionGroup",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-dimension": {
                                            "default": "0",
                                            "optionnames": ["Area", "Base", "Circumference", "Count", "Depth", "Diameter", "Height", "Length", "Please select a value", "Running Time", "Target", "Volume", "Weight", "Width"],
                                            "optionlist": ["", "area", "base", "circumference", "count", "depth", "diameter", "height", "length", "running_time", "target", "volume", "weight", "width"],
                                            "selection": "${{row}.dimension}"
                                        },
                                        ".csc-media-valueDate-container": {
                                            "decorators": [{
                                                "func": "cspace.datePicker",
                                                "type": "fluid"
                                            }]
                                        },
                                        ".csc-media-measurementUnit": {
                                            "default": "0",
                                            "optionnames": ["Carats", "Centimeters", "Cubic Centimeters", "Feet", "Inches", "Kilograms", "Liters", "Meters", "Millimeters", "Minutes", "Pixels", "Please select a value", "Square Feet", "Stories"],
                                            "optionlist": ["", "carats", "centimeters", "cubic_centimeters", "feet", "inches", "kilograms", "liters", "meters", "millimeters", "minutes", "pixels", "square_feet", "stories"],
                                            "selection": "${{row}.measurementUnit}"
                                        },
                                        ".csc-media-measuredBy": {
                                            "decorators": [{
                                                "func": "cspace.autocomplete",
                                                "type": "fluid",
                                                "options": {
                                                    "queryUrl": "../../chain/media/autocomplete/measuredBy",
                                                    "vocabUrl": "../../chain/media/source-vocab/measuredBy"
                                                }
                                            }],
                                            "value": "${{row}.measuredBy}"
                                        },
                                        ".csc-media-measurementMethod": {
                                            "default": "0",
                                            "optionnames": ["Balance/Beam Scale", "Electronic Distance Measurement", "Goniometer", "Hydraulic or Pneumatic Scale", "Measuring Tape (cloth)", "Measuring Tape (metal)", "Microscopy (reticule)", "Odometer", "Optical Range Finder", "Osteometric Board", "Pacing Pedometer", "Please select a value", "Protractor", "Ruler", "Sliding Calipers", "Spreading Calipers", "Spring Scale", "Stadia/Transit", "Standard Mesh/Screen", "Taping/Thaining", "Theodolite/Total Station"],
                                            "optionlist": ["", "balance_beam_scale", "electronic_distance_measurement", "goniometer", "hydraulic_or_pneumatic_scale", "measuring_tape_cloth", "measuring_tape_metal", "microscopy_reticule", "odometer", "optical_range_finder", "osteometric_board", "pacing_pedometer", "protractor", "ruler", "sliding_calipers", "spreading_calipers", "spring_scale", "stadia_transit", "standard_mesh_screen", "taping_chaining", "theodolite_total_station"],
                                            "selection": "${{row}.measurementMethod}"
                                        },
                                        ".csc-media-value": "${{row}.value}",
                                        ".csc-media-valueQualifier": "${{row}.valueQualifier}",
                                        ".csc-media-valueDate": "${{row}.valueDate}"
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.dimensionGroup"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-blobs-id": "${fields.blobs.0.id}",
        ".csc-media-blobCsid": "${fields.blobCsid}",
        ".csc-media-title": "${fields.title}",
        ".csc-blobs-tenantID": "${fields.blobs.0.tenantID}",
        ".csc-blobs-data": "${fields.blobs.0.data}",
        ".csc-media-identificationNumber": "${fields.identificationNumber}",
        ".csc-media-filename": "${fields.filename}",
        ".csc-blobs-imgMedium": "${fields.blobs.0.imgMedium}",
        ".csc-media-description": "${fields.description}",
        ".csc-media-createdAt": "${fields.createdAt}",
        ".csc-blobs-length": "${fields.blobs.0.length}",
        ".csc-media-dimensionSummary": "${fields.dimensionSummary}",
        ".csc-media-uri": "${fields.uri}",
        ".csc-media-publisher": {
            "decorators": [{
                "func": "cspace.autocomplete",
                "type": "fluid",
                "options": {
                    "queryUrl": "../../chain/media/autocomplete/publisher",
                    "vocabUrl": "../../chain/media/source-vocab/publisher"
                }
            }],
            "value": "${fields.publisher}"
        },
        ".csc-media-language": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.languages",
                    "protoTree": {
                        "expander": {
                            "tree": {
                                "expander": {
                                    "repeatID": "repeat:",
                                    "tree": {
                                        ".csc-media-language": {
                                            "optionnames": ["Ancient Greek", "Arabic", "Chinese", "English", "French", "German", "Hebrew", "Italian", "Japanese", "Korean", "Latin", "Middle English", "Old English", "Please select a value", "Portuguese", "Russian", "Spanish", "Swahili"],
                                            "optionlist": ["", "ancientgreek", "arabic", "chinese", "eng", "french", "german", "hebrew", "italian", "japanese", "korean", "latin", "middleenglish", "oldenglish", "portuguese", "russian", "spn", "swahili"],
                                            "selection": "${{row}.language}"
                                        }
                                    },
                                    "type": "fluid.renderer.repeat",
                                    "pathAs": "row",
                                    "controlledBy": "fields.languages"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                }
            }]
        },
        ".csc-media-identificationNumber-container": {
            "decorators": [{
                "func": "cspace.numberPatternChooser",
                "type": "fluid",
                "options": {
                    "model": {
                        "names": ["Media Resource"],
                        "list": ["media"],
                        "samples": ["MR2011.1.1"]
                    },
                    "selectors": {
                        "numberField": ".csc-media-identificationNumber"
                    }
                }
            }]
        }
    }
}