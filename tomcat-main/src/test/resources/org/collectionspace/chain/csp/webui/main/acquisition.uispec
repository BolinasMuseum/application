{
    "sidebar": {
        "relatedProcedures": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-summary": "${items.0.summary}",
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    },
                    ".csc-related-recordtype": "${items.0.recordtype}"
                }]
            }
        },
        "termsUsed": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    },
                    ".csc-related-recordtype": "${items.0.recordtype}",
                    ".csc-related-field": "${items.0.sourceFieldName}"
                }]
            }
        },
        "relatedCataloging": {
            ".csc-recordList-row:": {
                "children": [{
                    ".csc-related-summary": "${items.0.summary}",
                    ".csc-related-number": {
                        "linktext": "${items.0.number}",
                        "target": "${items.0.recordtype}.html?csid=${items.0.csid}"
                    }
                }]
            }
        }
    },
    "recordEditor": {
        ".csc-acquisition-object-purchase-price-currency": {
            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
            "selection": "${fields.objectPurchasePriceCurrency}"
        },
        ".csc-acquisition-original-object-purchase-price-value": "${fields.originalObjectPurchasePriceValue}",
        ".csc-acquisition-acquisitionFunding": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.acquisitionFunding",
                    "protoTree": {
                        ".csc-acquisition-acquisitionFundingSourceProvisos": "${fields.acquisitionFunding.0.acquisitionFundingSourceProvisos}",
                        ".csc-acquisition-acquisitionFundingSource": {
                            "valuebinding": "${fields.acquisitionFunding.0.acquisitionFundingSource}",
                            "decorators": [{
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/acquisition/autocomplete/acquisitionFundingSource",
                                    "vocabUrl": "../../chain/acquisition/source-vocab/acquisitionFundingSource"
                                }
                            }]
                        },
                        ".csc-acquisition-acquisitionFundingValue": "${fields.acquisitionFunding.0.acquisitionFundingValue}",
                        ".csc-acquisition-acquisitionFundingCurrency": {
                            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
                            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
                            "selection": "${fields.acquisitionFunding.0.acquisitionFundingCurrency}"
                        }
                    }
                }
            }]
        },
        ".csc-acquisition-acquisitionAuthorizerDate-container": {
            "decorators": [{
                "func": "cspace.datePicker",
                "type": "fluid"
            }]
        },
        ".csc-acquisition-acquisition-reason": "${fields.acquisitionReason}",
        ".csc-acquisition-object-purchase-price-value": "${fields.objectPurchasePriceValue}",
        ".csc-acquisition-acquisitionAuthorizer": {
            "valuebinding": "${fields.acquisitionAuthorizer}",
            "decorators": [{
                "func": "cspace.autocomplete",
                "type": "fluid",
                "options": {
                    "queryUrl": "../../chain/acquisition/autocomplete/acquisitionAuthorizer",
                    "vocabUrl": "../../chain/acquisition/source-vocab/acquisitionAuthorizer"
                }
            }]
        },
        ".csc-acquisition-acquisition-provisos": "${fields.acquisitionProvisos}",
        ".csc-acquisition-object-offer-price-value": "${fields.objectOfferPriceValue}",
        ".csc-object-identification-object-nametitle": "${fields.ownersource}",
        ".csc-acquisition-numberPatternChooser-reference-number": "${fields.acquisitionReferenceNumber}",
        ".csc-acquisition-object-offer-price-currency": {
            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
            "selection": "${fields.objectOfferPriceCurrency}"
        },
        ".csc-acquisition-object-purchase-offer-price-currency": {
            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
            "selection": "${fields.objectPurchaseOfferPriceCurrency}"
        },
        ".csc-acquisition-fieldCollectionEventName": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.fieldCollectionEventNames",
                    "protoTree": {
                        ".csc-acquisition-fieldCollectionEventName": "${fields.fieldCollectionEventNames.0.fieldCollectionEventName}"
                    }
                }
            }]
        },
        ".csc-acquisition-original-object-purchase-price-currency": {
            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
            "selection": "${fields.originalObjectPurchasePriceCurrency}"
        },
        ".csc-acquisition-acquisition-method": {
            "default": "0",
            "optionnames": ["Gift", "Purchase", "Exchange", "Transfer", "Treasure"],
            "optionlist": ["gift", "purchase", "exchange", "transfer", "treasure"],
            "selection": "${fields.acquisitionMethod}"
        },
        ".csc-acquisition-owner": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.owners",
                    "protoTree": {
                        ".csc-acquisition-owner": {
                            "valuebinding": "${fields.owners.0.owner}",
                            "decorators": [{
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/acquisition/autocomplete/owner",
                                    "vocabUrl": "../../chain/acquisition/source-vocab/owner"
                                }
                            }]
                        }
                    }
                }
            }]
        },
        ".csc-acquisition-accession-date": "${fields.accessionDate}",
        ".csc-acquisition-group-purchase-price-currency": {
            "optionnames": ["Please select a value", "US Dollar", "Swiss Franc", "Euro", "Canadian Dollar", "Danish Krone", "Swedish Krona", "Pound Sterling"],
            "optionlist": ["", "usd", "swissfranc", "eur", "canadiandollar", "danishkrone", "swedishkrona", "gbp"],
            "selection": "${fields.groupPurchasePriceCurrency}"
        },
        ".csc-acquisition-group-purchase-price-value": "${fields.groupPurchasePriceValue}",
        ".csc-acquisition-acquisitionSource": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.acquisitionSources",
                    "protoTree": {
                        ".csc-acquisition-acquisitionSource": {
                            "valuebinding": "${fields.acquisitionSources.0.acquisitionSource}",
                            "decorators": [{
                                "func": "cspace.autocomplete",
                                "type": "fluid",
                                "options": {
                                    "queryUrl": "../../chain/acquisition/autocomplete/acquisitionSource",
                                    "vocabUrl": "../../chain/acquisition/source-vocab/acquisitionSource"
                                }
                            }]
                        }
                    }
                }
            }]
        },
        ".csc-acquisition-reference-number-patternChooserContainer": {
            "decorators": [{
                "func": "cspace.numberPatternChooser",
                "type": "fluid",
                "options": {
                    "model": {
                        "names": ["Accession", "Archives", "Library"],
                        "list": ["accession", "archive", "library"],
                        "samples": ["2009.1", "AR2009.42", "LIB2009.1"]
                    },
                    "selectors": {
                        "numberField": ".csc-acquisition-numberPatternChooser-reference-number"
                    }
                }
            }]
        },
        ".csc-acquisition-acquisition-note": "${fields.acquisitionNote}",
        ".csc-acquisition-acquisitionAuthorizerDate": "${fields.acquisitionAuthorizerDate}",
        ".csc-acquisition-accession-date-container": {
            "decorators": [{
                "func": "cspace.datePicker",
                "type": "fluid"
            }]
        },
        ".csc-acquisition-object-purchase-offer-price-value": "${fields.objectPurchaseOfferPriceValue}",
        ".csc-acquisition-transfer-of-title-number": "${fields.transferOfTitleNumber}",
        ".csc-acquisition-acquisitionDates": {
            "decorators": [{
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "elPath": "fields.acquisitionDates",
                    "protoTree": {
                        ".csc-acquisition-date": "${fields.acquisitionDates.0.acquisitionDate}",
                        ".csc-acquisition-acquisition-date-container": {
                            "decorators": [{
                                "func": "cspace.datePicker",
                                "type": "fluid"
                            }]
                        }
                    }
                }
            }]
        }
    },
    "titleBar": {
        ".csc-titleBar-acquisition-reference-number": "${fields.acquisitionReferenceNumber}"
    }
}