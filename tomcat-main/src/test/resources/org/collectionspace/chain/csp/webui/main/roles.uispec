{
    "details": {
        ".csc-role-name": "${fields.roleName}",
        ".csc-role-description": "${fields.description}",
        ".csc-role-group": "${fields.roleGroup}",
        ".csc-permrole-role": {
            "children": [{}]
        },
        ".csc-permrole-permission": {
            "children": [{
                ".csc-permrole-permissionId": "${fields.permission.0.permissionId}",
                ".csc-permrole-resourceName": "${fields.permission.0.resourceName}"
            }]
        }
    },
    "list": {
        ".csc-recordList-row:": {
            "children": [{
                ".csc-user-roleList-name": "${items.0.number}",
                ".csc-user-userList-csid": "${items.0.csid}"
            }]
        }
    }
}