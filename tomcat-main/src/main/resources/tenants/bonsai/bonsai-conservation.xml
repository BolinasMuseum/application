<record id="conservation">
  <section id="objectTreatmentInformation">
    <repeat id="treatmentsPerformed">
      <field id="treatmentPerformed" ui-search="repeatable" autocomplete="vocab-treatmentperformed" ui-type="enum"/>
    </repeat>
  </section>

  <section id="objectAnalysisInformation">
    <repeat id="futureTreatmentGroupList/futureTreatmentGroup">
      <field id="futureTreatment" autocomplete="vocab-futuretreatment" ui-type="enum"/>
      <field id="futureTreatmentDate" ui-search="range" ui-type="date" datatype="date"/>
      <field id="futureTreatmentNote"/>
    </repeat>
  </section>
</record>